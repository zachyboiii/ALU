// A full adder combinational logic unit 
module fa (
    input a,
    input b,
    input cin,
    output s,
    output cout
) {
    
    always {
        // implement 1-bit full adder logic here
        s = a ^ b ^ cin // ^ -> exclusive or
        cout = (a & b) + (a & cin) + (b & cin) 
        
    }
    
}